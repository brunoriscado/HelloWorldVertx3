                "lowercase",
                "stemmer_filter_str"
              ]
            },
            "uni_noStem": {
              "type": "custom",
              "tokenizer": "whitespace",
              "filter": ["lowercase"]
            },
            "bi_str_sym": {
              "tokenizer": "standard",
              "filter": [
                "lowercase",
                "shingle_filter",
                "synonym_ans",
                "stemmer_filter_str"
              ]
            },
            "uni_str_sym": {
              "type": "custom",
              "tokenizer": "whitespace",
              "filter": [
                "lowercase",
                "synonym_ans",
                "stemmer_filter_str"
              ]
            }
          },
          "filter": {
            "synonym_ans": {
              "tokenizer": "keyword",
              "type": "synonym",
              "synonyms_path": "synonyms/synonyms.txt"
            },
            "shingle_filter": {
              "type": "shingle",
              "min_shingle_size": 2,
              "max_shingle_size": 2,
              "output_unigrams": false
            },
            "concat_filter": {
              "type": "shingle",
              "min_shingle_size": 2,
              "max_shingle_size": 2,
              "output_unigrams": false,
              "token_separator": ""
            },
            "stemmer_filter_str": {
              "type": "stemmer",
              "name": "minimal_english"
            }
          }
        }
      }
